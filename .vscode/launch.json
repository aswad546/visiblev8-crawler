    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/celery_workers/vv8_worker/vv8_crawler/crawler.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["visit", "'https://interactivefidemo.nudatasecurity.com/signin'", "12345"], 
            "type": "node"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        }
    ]
}